{
  "name": "@zeytech/cache-adonisjs",
  "version": "1.0.16",
  "description": "Provides LRU and Timed LRU in-memory cache with support for Adonis",
  "main": "build/providers/CacheProvider.js",
  "repository": {
    "url": "git://github.com/zeytechinc/cache-adonisjs.git"
  },
  "keywords": [
    "adonisjs",
    "cache"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc && npm run copyfiles",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json zeytech/cache-adonisjs",
    "copyfiles": "copyfiles \"templates/**/*.txt\" \"instructions.md\" build"
  },
  "author": "Zeytech Inc (https://zeytech.com)",
  "contributors": [
    "Steve Krenek (https://github.com/skrenek)"
  ],
  "license": "MIT",
  "typings": "./build/adonis-typings/index.d.ts",
  "devDependencies": {
    "@adonisjs/core": "^5.4.2",
    "@adonisjs/mrm-preset": "^5.0.2",
    "@adonisjs/redis": "^7.1.0",
    "@adonisjs/require-ts": "^2.0.9",
    "@adonisjs/sink": "^5.2.1",
    "@types/node": "^14.14.37",
    "commitizen": "^4.2.3",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^3.0.1",
    "doctoc": "^2.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-adonis": "^1.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "github-label-sync": "^2.0.0",
    "husky": "^6.0.0",
    "japa": "^3.1.1",
    "luxon": "^1.26.0",
    "mrm": "^2.6.2",
    "np": "^7.4.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "doctoc README.md --title='## Table of contents' && git add README.md",
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/src",
    "build/templates",
    "build/instructions.*",
    "build/standalone.js",
    "build/standalone.d.ts"
  ],
  "adonisjs": {
    "types": "@zeytech/cache-adonisjs",
    "providers": [
      "@zeytech/cache-adonisjs"
    ]
  }
}
